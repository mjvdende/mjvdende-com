<header>
    <img src="<%- theme.profile_pic %>" style="margin-top:2em;float:right;margin-right:0rem;margin-bottom:0;clip-path:circle(125px at center);">
    <h1><%- theme.subtitle_main %></h1>
    <p><%- theme.susbtitle_secondary %></p>
</header>

<section class="tiles">
    <% page.posts.forEach(function(item, index){ %>
        <%- partial('_partial/article-index', {item: item, index: index}) %>
    <% }); %>
</section>

<%- partial('_partial/pagination') %>
